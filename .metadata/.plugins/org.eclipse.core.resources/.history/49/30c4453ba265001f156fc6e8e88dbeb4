<script type="text/javascript" src="http://code.jquery.com/jquery-3.7.1.min.js"></script>
<script type="text/javascript">
$(function() {
	$('#mybtn').click(function() {
		$.ajax({
			type:'get',
			url: '../json/json01.json',
			dataType: 'json',
			success: function(data) {
				
			},
			error: function(e) {
				console.log(e);
			}
		});
	});
});

</script>

